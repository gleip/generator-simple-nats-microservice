{
  "extends": [
    "tslint-config-airbnb",
    "tslint-eslint-rules"
  ],
  "rules": {
    "trailing-comma": [
      true,
      {
        "multiline": {
          "objects": "always",
          "arrays": "always",
          "functions": "never",
          "typeLiterals": "ignore"
        },
        "esSpecCompliant": true
      }
    ]
  }
}